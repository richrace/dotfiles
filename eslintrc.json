{
  "parserOptions": {
    "ecmaVersion": 5,
    "sourceType": "module",
    "ecmaFeatures": {
     "jsx": true
    }
  },
  "rules": {
    "semi": 2,
    "quotes": 0,
    "eqeqeq": 2,
    "react/prop-types": 1,
    "no-eval": 2
  },
  "plugins": [
    "react"
  ],
  "extends": [
    "eslint:recommended",
    "plugin:react/recommended"
  ],
  "env": {
    "browser": true,
    "jquery": true
  },
  "globals": {
    "_": true,
    "React": true,
    "Redux": true,
    "ReactRedux": true,
    "ReactCSSTransitionGroup": true
  }
}
